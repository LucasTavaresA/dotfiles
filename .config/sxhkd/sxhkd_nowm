#### INDENPENDENTE ####
# Terminal #
# st
super + shift + Return
    st
# abre e fecha scratchpads
super + backslash
    st_scratchpad

# Musica #
# pausa/toca musica
super + space
    playerctl play-pause || mpc toggle
# anterior/proxima musica
super + shift + {comma, period}
    mpc {prev,next} && musica notificar
# abaixar/aumentar o volume
super + {comma, period}
    amixer -q set Master 5%{-,+,-,+}
# ncmpcpp/pulsemixer
super + {_,shift} + a
    st -c {ncmpcpp -n ncmpcpp -e ncmpcpp,pulsemixer -n pulsemixer -e pulsemixer}


# Menus #
# fzf_emoji
Menu;E
    st -c fzf -n fzf -e fzf_emojis
# fzf_man
Menu;m
    st -e fzf_man
# fzf_sxhkd
Menu;slash
    st -c fzf -n fzf -e fzf_sxhkd
# fzf_clip
Menu;c
    st -c fzf -n fzf -e fzf_clip
# passmenu
Menu;p
    passmenu --type
# dmenu_run
Menu;Menu
    dmenu_run
# dmenu_sys
{Menu;Escape,XF86Launch2}
    dmenu_sys
# dmenu_mont
Menu;M
    dmenu_mont
# dmenu_wifi
Menu;w
    dmenu_wifi
# dmenu_edit
Menu;e
    dmenu_edit
# dmenu_browser
Menu;q
    dmenu_browser ~/code/shell/listas/favoritos.yaml
# dmenu_xephyr
Menu;x
    dmenu_xephyr
# dmenu_print
Print
    dmenu_print

# Outros #
# htop
Menu;h
    st -c htop -n htop -e htop
# picom
Menu;P
    pkill picom || picom
# sct
Menu;{f,F}
    {sct,sct 2000}
# calculadora
Menu;C
    galculator
# transmission
Menu;t
    transmission-qt
# anotações
Menu;a
    $VISUAL ~/documentos/anotações.md
