#### INDENPENDENTE ####
# Terminal #
# st
super + shift + Return
    st
# abre e fecha scratchpads
super + backslash
    st_scratchpad

# Musica #
# pausa/toca musica
super + space
    playerctl play-pause || mpc toggle
# anterior/proxima musica
super + shift + {comma, period}
    mpc {prev,next} && musica notificar
# abaixar/aumentar o volume
super + {comma, period}
    amixer -q set Master 5%{-,+,-,+}
# ncmpcpp/pulsemixer
super + {_,shift} + a
    st -g 120x30 -c {ncmpcpp -n ncmpcpp -e ncmpcpp,pulsemixer -n pulsemixer -e pulsemixer}

# Dmenus #
# dmenu_run
Menu;Menu
    dmenu_run
# dmenu_sys
{Menu;Escape,XF86Launch2}
    dmenu_sys
# dmenu_wifi
Menu;w
    dmenu_wifi
# dmenu_edit
Menu;e
    dmenu_edit
# dmenu_emoji
Menu;E
    dmenu_emoji
# dmenu_mont
Menu;m
    dmenu_mont
# dmenu_sxhkd
Menu;slash
    dmenu_sxhkd
# dmenu_shhistory
Menu;h
    dmenu_shhistory
# passmenu
Menu;p
    passmenu --type
# dmenu_clip
Menu;c
    dmenu_clip
# dmenu_qutebrowser
Menu;q
    dmenu_qutebrowser ~/code/shell/dmenuscripts/listas/favoritos.yaml
# dmenu_aliases
Menu;A
    dmenu_aliases
# dmenu_xephyr
Menu;x
    dmenu_xephyr
# dmenu_print
Print
    dmenu_print

# Outros #
# htop
Menu;H
    st -g 120x30 -c htop -n htop -e htop
# picom
Menu;P
    killall picom || picom
# fluxgui
Menu;F
    killall fluxgui || fluxgui
# calculadora
Menu;C
    galculator
# transmission
Menu;t
    transmission-gtk
# anotações
Menu;a
    nvim ~/documentos/anotações.md
